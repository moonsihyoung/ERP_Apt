<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>공동주택 관리전산</title>   
    @*<link href="~/css/bootstrap.css" rel="stylesheet" />*@
    <link rel="stylesheet" href="~/css/bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/siteMVC.css" />
    <link href="~/icons-1.10.3/font/bootstrap-icons.css" rel="stylesheet" />
    @*<link href="~/icons-1.3.0/font/bootstrap-icons.css" rel="stylesheet" />*@
    @*<link href="~/bootstrap-icons-1.8.1/bootstrap-icons.css" rel="stylesheet" />*@
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="/images/Scw.Jpg" class="rounded" height="30" width="30" alt="" /> 공동주택 관리전산</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index"><span class="bi bi-house-door-fill"></span>&nbsp;Home</a>
                        </li>
                        
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 신원티엠씨(TMC)
        </div>
    </footer>


    <script src="~/js/jquery/jquery.min.js"></script>
    <script src="~/js/jquery/jquery-ui.min.js"></script>
    <script src="~/css/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)


</body>
</html>
